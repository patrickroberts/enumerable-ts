{
  "name": "enumerable-ts",
  "version": "0.0.1-beta",
  "description": "A port of System.Linq.Enumerable from the .NET framework to TypeScript",
  "directories": {
    "doc": "docs",
    "lib": "src"
  },
  "main": "src/global.js",
  "scripts": {
    "prepublishOnly": "npm run build && npm run lint",
    "build": "tsc -p .",
    "lint": "tslint -p ./tsconfig.json"
  },
  "keywords": [
    "enumerable",
    "iterable",
    "iterator",
    "lazy",
    "interface",
    "extension"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickroberts/enumerable-ts.git"
  },
  "author": "Patrick Roberts",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.12.0"
  },
  "devDependencies": {
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3"
  }
}
